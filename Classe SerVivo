package starWars;

public abstract class SerVivo extends Objeto{
	protected int anoDeNascimento;
	protected String epocaDeNascimento;
	protected String localDeNascimento;
	protected String sexo;
	protected String especie;
	
	SerVivo(int data, String nom, String sex, String esp, String epoch, String loc) {
		super(nom);
		anoDeNascimento = data;
		sexo = sex;
		especie = esp;
		epocaDeNascimento = epoch;
		localDeNascimento = loc;
	}
	
}
